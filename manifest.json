{
    "manifest_version": 2,
    "name": "WolverineEats",
    "version": "0.2",
    "description": "Check the menu of UMich Dining",
    "author": "zelongjiang<zelong@umich.edu>",
    "browser_action": {
        "default_icon": "sausage.png"
    },
    "background": {
        "scripts": [
            "/background.js"
        ]
    },
    "devtools_page": "devtools.html",
    "permissions": [
        "storage",
        "tabs",
        "https://dining.umich.edu/menus-locations/dining-halls/*"
    ]
}